{
  "routes": [
    { 
      "handle": "filesystem"
    },
    {
      "src": "/assets/.*\\.js$",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/assets/.*\\.css$",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"

    }
  ],
  "cleanUrls": true
} 
